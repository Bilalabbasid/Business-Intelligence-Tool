{
  "dashboard": {
    "id": null,
    "title": "Data Quality Monitoring Dashboard",
    "tags": ["data-quality", "bi-platform", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "DQ System Health",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "dq_system_health",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Active Rules",
        "type": "stat",
        "gridPos": {"h": 4, "w": 3, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "dq_active_rules_count",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Recent Violations",
        "type": "stat",
        "gridPos": {"h": 4, "w": 3, "x": 9, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(dq_violations_count[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "Avg Check Duration",
        "type": "stat",
        "gridPos": {"h": 4, "w": 3, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "avg(dq_check_duration)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 60}
              ]
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 5,
        "title": "DQ Check Success Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(dq_checks_success_total[5m]) / rate(dq_checks_total[5m])",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "percentunit",
            "min": 0.8,
            "max": 1.0,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.9},
                {"color": "green", "value": 0.95}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 6,
        "title": "Violations by Severity",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum by (severity) (dq_violations_count)",
            "legendFormat": "{{severity}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"hideFrom": {"legend": false, "tooltip": false, "vis": false}},
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "CRITICAL"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            },
            {
              "matcher": {"id": "byName", "options": "HIGH"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
            },
            {
              "matcher": {"id": "byName", "options": "MEDIUM"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "yellow"}}]
            }
          ]
        },
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "pieType": "pie",
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "list", "placement": "right", "values": ["value"]}
        }
      },
      {
        "id": 7,
        "title": "Check Execution Duration",
        "type": "histogram",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(dq_check_duration_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(dq_check_duration_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 8,
        "title": "Top Failing Rules",
        "type": "table",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "topk(10, sum by (rule_name) (increase(dq_check_failures_total[24h])))",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "rule_name"},
              "properties": [{"id": "displayName", "value": "Rule Name"}]
            },
            {
              "matcher": {"id": "byName", "options": "Value"},
              "properties": [{"id": "displayName", "value": "Failures (24h)"}]
            }
          ]
        },
        "options": {
          "showHeader": true
        }
      },
      {
        "id": 9,
        "title": "Data Freshness SLA",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "avg by (database) (dq_data_freshness_hours)",
            "legendFormat": "{{database}} - Freshness (hours)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "Hours",
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 10
            },
            "unit": "h",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 6}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 10,
        "title": "Worker Queue Length",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(celery_queue_length{queue=~\"dq.*\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            },
            "unit": "short",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""},
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 11,
        "title": "Alert Response Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(dq_alert_response_time_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "avg(dq_alert_response_time)",
            "legendFormat": "Average",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "m",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 15},
                {"color": "red", "value": 60}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Recent DQ Events",
        "type": "logs",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "{job=\"django-dq\"}",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "sortOrder": "Descending"
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "DQ Deployments",
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "type": "dashboard"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "database",
          "type": "query",
          "query": "label_values(dq_checks_total, database)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "options": [],
          "current": {"selected": false, "text": "All", "value": "$__all"}
        },
        {
          "name": "severity",
          "type": "custom",
          "options": [
            {"text": "All", "value": "$__all", "selected": true},
            {"text": "CRITICAL", "value": "CRITICAL", "selected": false},
            {"text": "HIGH", "value": "HIGH", "selected": false},
            {"text": "MEDIUM", "value": "MEDIUM", "selected": false},
            {"text": "LOW", "value": "LOW", "selected": false}
          ],
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "current": {"selected": false, "text": "All", "value": "$__all"}
        }
      ]
    }
  }
}